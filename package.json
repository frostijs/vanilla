{
  "name": "@frosti/vanilla",
  "version": "0.2.0",
  "description": "Boilerplate Vanilla JS App",
  "author": "Jesse Weed <code@jesseweed.com>",
  "license": "MIT",
  "homepage": "http://frosti.js.org",
  "repository": "https://github.com/frostijs/vanilla",
  "bugs": "https://github.com/frostijs/vanilla/issues",
  "engines": {
    "node": "10.15.x"
  },
  "scripts": {
    "build:config": "cd ../packages/config/ && npm run build && cd ../../vanilla",
    "build:all": "run-s build:config build:client",
    "build": "run-p build:client build:server",
    "build:client": "npm run rollup:prod",
    "build:server": "npm run rollup:server",
    "build:dev": "npm run rollup:watch",
    "check": "run-s check:unused check:security",
    "check:unused": "node lib/util/depcheck",
    "check:security": "npm audit",
    "copy": "node lib/util/copy",
    "deploy": "run-p copy deploy:firebase",
    "deploy:firebase": "NODE_ENV=production firebase deploy --only functions,hosting",
    "dev": "run-p build:dev start:node",
    "docker": "run-s docker:build docker:start",
    "docker:build": "docker build -t frosti-react ./",
    "docker:start": "docker run -it -p 3000:3000 frosti-react",
    "lint": "eslint src --ext .js,.jsx --ignore-path .gitignore .",
    "prettier": "pretty-quick",
    "reload": "livereload '.dist/' -d",
    "rollup:prod": "NODE_ENV=production rollup -c config/rollup/rollup.config.js",
    "rollup:server": "NODE_ENV=production rollup -c config/rollup/rollup.config.server.js",
    "rollup:watch": "NODE_ENV=development rollup -c config/rollup/rollup.config.js -w",
    "start:heroku": "HOST=heroku node ./functions/.dist/ssr.js",
    "start:docker": "HOST=docker node ./functions/.dist/ssr.js",
    "start:node": "HOST=local babel-node src/Server.js",
    "start:ssr": "HOST=ssr node ./functions/.dist/ssr.js",
    "ssl": "nodecert",
    "test": "jest --config ./config/jest.config.js",
    "vet": "run-p prettier check test",
    "r": "NODE_ENV=production babel-node eslintrc.js"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run vet"
    }
  },
  "dependencies": {
    "chokidar": "^3.0.0",
    "colors": "^1.3.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "history": "^4.9.0",
    "path-to-regexp": "^3.0.0",
    "redirect-https": "^1.3.0",
    "spdy": "^4.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.x",
    "@babel/core": "^7.4.x",
    "@babel/node": "^7.4.x",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.x",
    "@babel/plugin-syntax-dynamic-import": "^7.2.x",
    "@babel/plugin-syntax-jsx": "^7.2.x",
    "@babel/plugin-transform-react-jsx": "^7.3.x",
    "@babel/plugin-transform-react-jsx-source": "^7.2.x",
    "@babel/plugin-transform-runtime": "^7.4.x",
    "@babel/preset-env": "^7.4.x",
    "@csstools/postcss-sass": "^4.0.x",
    "babel-plugin-module-resolver": "^3.2.x",
    "babel-plugin-transform-postcss": "^0.3.0",
    "depcheck": "^0.8.x",
    "eslint": "^5.3.x",
    "eslint-config-airbnb-base": "^13.1.x",
    "eslint-import-resolver-babel-module": "^5.1.x",
    "eslint-plugin-import": "^2.17.x",
    "favicons": "^5.4.x",
    "fs-extra": "^8.0.x",
    "npm-run-all": "^4.1.x",
    "postcss-easy-import": "^3.0.x",
    "postcss-hexrgba": "^1.0.x",
    "postcss-mixins": "^6.2.x",
    "postcss-preset-env": "^6.6.x",
    "postcss-pxtorem": "^4.0.x",
    "rimraf": "^2.6.x",
    "rollup": "^1.12.x",
    "rollup-plugin-babel": "^4.3.x",
    "rollup-plugin-babel-minify": "^8.0.x",
    "rollup-plugin-browsersync": "^1.0.x",
    "rollup-plugin-commonjs": "^10.0.x",
    "rollup-plugin-eslint": "^6.0.x",
    "rollup-plugin-import-alias": "^1.0.7",
    "rollup-plugin-json": "^4.0.x",
    "rollup-plugin-node-resolve": "^5.0.x",
    "rollup-plugin-postcss": "^2.0.x",
    "rollup-plugin-replace": "^2.2.x",
    "rollup-plugin-size-snapshot": "^0.9.x",
    "rollup-plugin-uglify": "^6.0.x"
  }
}
